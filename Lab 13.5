setlocale(LC_ALL, "Russian");
	int N;
	cout << "Введите N: ";
	cin >> N;
	int *A = new int[N];
	for (int i = 0; i < N; i++)
		cin >> A[i];

	for (int i = 1; i < N; i += 2)
		cout << A[i] << " ";

	if (N % 2 == 0) {
		for (int i = N - 2; i >= 0; i -= 2)
			cout << A[i] << " ";
	}
	else {
		for (int i = N - 1; i >= 0; i -= 2)
			cout << A[i] << " ";
	}
	return 0;
