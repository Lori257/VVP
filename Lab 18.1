setlocale(LC_ALL, "Russian");
	int m, i, j, k;
    cout << "Введите m: ";
	cin >> m;
	int** A = new int* [m];
	for (i = 0; i < m; i++) {
		A[i] = new int[m];
	}
	cout << "Введите элементы матрицы: ";
	for (i = 0; i < m; i++) {
		for (j = 0; j < m; j++) {
			cin >> A[i][j];
		}
	}
	for (k = 0; k < (m / 2 + 1); k++)
	{
		for (i = k; i < m - k; i++)
		{
			cout << A[i][k] << " ";
		}
		i=i-1;
		for (j = k + 1; j < m - k; j++)
		{
			cout << A[i][j] << " ";
		}
		j=j-1;
		for (i = m - k - 2; i >= k; i--)
		{
			cout << A[i][j] << " ";
		}
		i=i+1;
		for (j = m - k - 2; j > k; j--)
		{
			cout << A[i][j] << " ";
		}
		j=j+1;
	}
	return 0;
